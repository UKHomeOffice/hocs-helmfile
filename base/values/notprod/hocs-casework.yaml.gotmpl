---
hocs-generic-service:
  version: {{ .Values.versions.hocs_casework }}

  isProd: false

  autoscale:
    enabled: false

  proxy:
    enabled: false

  deployment:
    replicas: 1
    annotations:
      downscaler/downtime: "Mon-Sun 00:00-07:55 Europe/London,Mon-Sun 18:05-24:00 Europe/London"

  app:
    resources:
      limits:
        memory: 768Mi
      requests:
        memory: 768Mi
