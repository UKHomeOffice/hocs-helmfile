---
hocs-generic-service:
  version: {{ .Values.versions.hocs_frontend }}

  ingress:
    external:
      allowedIps: {{ env "ALLOW_LIST" }}

{{- if contains .Environment.Name "wcs-" }}
  clusterPrefix: wcs

  keycloak:
    realm: https://sso.digital.homeoffice.gov.uk/auth/realms/HOCS
    domain: wcs.homeoffice.gov.uk
{{- end }}
